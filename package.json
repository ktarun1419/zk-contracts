{
  "name": "zksync-hardhat-template",
  "description": "A template for zkSync smart contracts development with Hardhat",
  "private": true,
  "author": "Matter Labs",
  "license": "MIT",
  "repository": "https://github.com/matter-labs/zksync-hardhat-template.git",
  "scripts": {
    "deploy": "hardhat deploy-zksync --script deploy.ts",
    "deploy-factory": "hardhat deploy-zksync --script deployFactory.ts",
    "deploy-nft":"hardhat deploy-zksync --script deployNFT.ts",
    "deploy-mintclaim":"hardhat deploy-zksync --script deployMintWithClaim.ts",
    "deploy-test":"hardhat deploy-zksync --script deployTest.ts",
    "interact": "hardhat deploy-zksync --script interact.ts",
    "compile": "hardhat compile",
    "clean": "hardhat clean",
    "test": "hardhat test --network hardhat"
  },
  "devDependencies": {
    "@matterlabs/hardhat-zksync": "^0.1.0",
    "@matterlabs/hardhat-zksync-upgradable": "^1.4.1",
